# Ignore directories generated by Composer
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
# Ignore sensitive information
/web/sites/*/settings*.php
/web/sites/*/services*.yml
# Ignore default files directory as it contains uploaded files and private files
/web/sites/*/files/
# Ignore simpletest and PHP upgrade testing directories
/web/sites/simpletest/
/web/sites/*/private/
# Ignore configuration files that may contain sensitive information.
/sites/*/settings*.php
# Ignore files that should be private, such as your private key for ssh
/private/
# Ignore the Composer lock file and other composer related files
/composer.lock
/composer.phar
/composer/installers
/vendor/bin/
# Ignore Node.js build tools
node_modules/
# Ignore IDE specific files and OS generated files like .DS_Store
.idea/
*.swp
*.bak
*.tmp
.DS_Store
# Drush configuration and cache files.
/drush.yml
/drush/
# Custom scripts, dumps and so on.
/scripts/
/dumps/
# Ignore the files generated by the Devel module.
/web/sites/*/devel-generated/
# Ignore PHPStorm project files
.idea/
# Ignore SimpleTest multi-site environment.
/sites/simpletest/
# Ignore PHPunit configuration.
/phpunit.xml
# Ignore files generated by backup and migrate module.
/private/backups/
/private/config/
# Ignore automatic backup folders.
_backup/
# Ignore the .env file as it usually contains environment-specific information.
.env
